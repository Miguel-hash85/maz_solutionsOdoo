<odoo>
    <data>
        <record model="ir.ui.view" id="exam_form_view">
            <field name="name">maz_solutions_exam.form</field>
            <field name="model">maz_solutions.exam</field>
            <field name="arch" type="xml">
                <form string="Exam Form">
                    <sheet>
                        <group colspan="2" col="2">
                            <h1>Exam Creation</h1>    
                            <group>
                                <field name="statment"/>
                                <field name="subject"/>
                            </group>        
                        </group>
                       
                        <notebook>
                            <page string="Exam Session">
                                <field name="exam_sessions"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
